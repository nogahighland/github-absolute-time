{
  "manifest_version": 3,
  "name": "GitHub Great Time",
  "description": "Change the notation of ~hours/days ago displayed in GitHub issue/PR to absolute time notation.",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "jquery.min.js",
        "moment.js",
        "ja.js",
        "index.js"
      ],
      "run_at": "document_end"
    }
  ]
}
